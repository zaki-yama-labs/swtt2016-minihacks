<aura:component access="global">
  <aura:attribute name="contacts" type="Contact[]"/>

  <ul>
    <aura:iteration items="{!v.contacts}" var="contact">
    <li>
      <a href="{! '#/sObject/' + contact.Id + '/view'}">
        <p>{!contact.Name}</p>
        <p>{!contact.Account.Name}</p>
      </a>
    </li>
    </aura:iteration>
  </ul>
</aura:component>
